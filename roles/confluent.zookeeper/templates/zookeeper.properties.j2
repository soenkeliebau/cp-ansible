{% for key, value in zookeeper.config.items() %}
{{key}}={{value}}
{% endfor %}
{% for host in groups['zookeeper'] %}
{% set zookeeper_vars = hostvars[host].get('zookeeper', dict()) %}
{% set id = zookeeper_vars.get('id', loop.index) %}
server.{{ id }}={{ host }}:2888:3888
{% endfor %}
